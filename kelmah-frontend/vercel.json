{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://4a65b7df9cc5.ngrok-free.app/api/$1"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "https://09102012e1ed.ngrok-free.app/socket.io/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}