{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://ec2-16-170-242-190.eu-north-1.compute.amazonaws.com:3000/api/$1"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "http://ec2-16-170-242-190.eu-north-1.compute.amazonaws.com:3000/socket.io/$1"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}